<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserReportController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="User Login Attempts">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="518.0" layoutY="20.0" mnemonicParsing="false" onAction="#onBack" text="Back" />
      <Label layoutX="36.0" layoutY="103.0" text="Amount of login attempts  :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="300.0" layoutY="20.0" mnemonicParsing="false" onAction="#onCalculate" text="Calculate" />
      <Label fx:id="loginCount" layoutX="286.0" layoutY="108.0" />
   </children>
</AnchorPane>
